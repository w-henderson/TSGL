{
  "compilerOptions": {
    "declaration": true,
    "lib": [
      "es2020",
      "dom",
      "dom.iterable"
    ],
    "module": "es6",
    "moduleResolution": "Node16",
    "removeComments": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2020",
    "outDir": "./dist"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}